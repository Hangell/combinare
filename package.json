{
  "name": "combinare",
  "version": "1.0.0",
  "description": "A biblioteca \"combinare\" é uma ferramenta poderosa para combinar arrays e gerar possibilidades numéricas. ",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "cp package.json dist/package.json && cp README.md dist/README.md",
    "build": "webpack --config webpack.config.js && tsc -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx)\"",
    "commit": "git-cz"
  },
  "keywords": [],
  "author": "Rodrigo Rangel <rodrigo@hangell.org> (https://github.com/Hangell)",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "29.5.1",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard-with-typescript": "34.0.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "git-cz": "4.9.0",
    "jest-extended": "3.2.4",
    "prettier": "2.8.8",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.2",
    "ts-standard": "12.0.2",
    "typescript": "5.0.4",
    "webpack": "5.83.1",
    "webpack-cli": "5.1.1"
  },
  "ts-standard": {
    "project": "./tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hangell/combinare.git"
  },
  "bugs": {
    "url": "https://github.com/Hangell/combinare/issues"
  },
  "homepage": "https://github.com/Hangell/combinare#readme"
}
